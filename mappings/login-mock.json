{
  "request": {
    "method": "POST",
    "url": "/login",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.username == 'testUser' && @.password == 'testPass')]"
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "login-response.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
